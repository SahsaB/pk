<?xml version="1.0" encoding="utf-8"?>
<Template
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="..\Schemas\DocumentSchema.xsd">
  <Fonts>
    <Font ID="Usual">
      <Name>Arial</Name>
      <Size>16</Size>
    </Font>
    <Font ID="UsualBold">
      <Name>Arial</Name>
      <Size>16</Size>
      <Style>Bold</Style>
    </Font>
    <Font ID="Large">
      <Name>Arial</Name>
      <Size>22</Size>
    </Font>
    <Font ID="LargeBold">
      <Name>Arial</Name>
      <Size>22</Size>
      <Style>Bold</Style>
    </Font>
  </Fonts>
  <Document>
    <Word>
      <Properties>
        <A5/>
        <Album/>
      </Properties>
      <Structure>
        <Element>
          <FixedTable>
            <ColumnsParameters>
              <Columns>
                <ColumnWidth>550</ColumnWidth>
                <ColumnWidth>5000</ColumnWidth>
              </Columns>
            </ColumnsParameters>
            <Borders>
              <Left>
                <Style>None</Style>
              </Left>
              <Top>
                <Style>None</Style>
              </Top>
              <Right>
                <Style>None</Style>
              </Right>
              <Bottom>
                <Style>None</Style>
              </Bottom>
            </Borders>
            <Rows>
              <Row>
                <Cells>
                  <Cell Column="0">
                    <Borders>
                      <Right>
                        <Size>5</Size>
                        <Style>Dashed</Style>
                      </Right>
                    </Borders>
                    <Paragraphs>
                      <Paragraph>
                        <FontID>UsualBold</FontID>
                        <Parts>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrLastName|UPPERCASE</Placeholder>
                            </Text>
                          </Part>
                          <Part>
                            <Text>
                              <String>--</String>
                            </Text>
                          </Part>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrFirstName|FIRST_LETTER_DOT</Placeholder>
                            </Text>
                          </Part>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrMiddleName|FIRST_LETTER_DOT</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                    </Paragraphs>
                  </Cell>
                  <Cell Column="1">
                    <Borders>
                      <Left>
                        <Size>5</Size>
                        <Style>Dashed</Style>
                      </Left>
                    </Borders>
                    <Paragraphs>
                      <Paragraph>
                        <FontID>LargeBold</FontID>
                        <Alighment>Right</Alighment>
                        <Parts>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrantID</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                      <Paragraph>
                        <FontID>Usual</FontID>
                        <Alighment>Right</Alighment>
                        <Parts>
                          <Part>
                            <Text>
                              <String>!ФОРМА ОБУЧЕНИЯ!</String>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                      <Paragraph/>
                      <Paragraph/>
                      <Paragraph/>
                      <Paragraph>
                        <FontID>LargeBold</FontID>
                        <Alighment>Center</Alighment>
                        <Parts>
                          <Part>
                            <FontID>LargeBold</FontID>
                            <Text>
                              <Placeholder>applicationEntrLastName</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                      <Paragraph>
                        <FontID>Large</FontID>
                        <Alighment>Center</Alighment>
                        <Parts>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrFirstName</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                      <Paragraph>
                        <FontID>Large</FontID>
                        <Alighment>Center</Alighment>
                        <Parts>
                          <Part>
                            <Text>
                              <Placeholder>applicationEntrMiddleName</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                      <Paragraph>
                        <FontID>Large</FontID>
                        <Alighment>Center</Alighment>
                        <Parts>
                          <Part>
                            <Text>
                              <Placeholder>applicationTime|YEAR_ONLY</Placeholder>
                            </Text>
                          </Part>
                        </Parts>
                      </Paragraph>
                    </Paragraphs>
                  </Cell>
                </Cells>
              </Row>
            </Rows>
          </FixedTable>
        </Element>
      </Structure>
    </Word>
  </Document>
</Template>
