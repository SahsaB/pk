<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>
  <connectionStrings>
    <add name="pk_db" connectionString="server = localhost; port = 3306; database = pk_db;"/>
    <add name="kladr" connectionString="server = localhost; port = 3306; database = kladr;"/>
  </connectionStrings>
  <appSettings>
    <add key="DocumentsTemplatesPath" value=".\DocumentTemplates\"/>
    <add key="SchemasPath" value=".\Schemas\"/>
    <add key="FIS_Login" value="XXXX"/>
  </appSettings>
</configuration>
