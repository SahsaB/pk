<?xml version="1.0" encoding="utf-8"?>
<Template
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="..\Schemas\DocumentSchema.xsd">
  <Fonts>
    <Font ID="Usual">
      <Name>Arial</Name>
      <Size>14</Size>
    </Font>
    <Font ID="UsualBold">
      <Name>Arial</Name>
      <Size>14</Size>
      <Style>Bold</Style>
    </Font>
    <Font ID="UsualTable">
      <Name>Arial</Name>
      <Size>10</Size>
    </Font>
    <Font ID="UsualBoldTable">
      <Name>Arial</Name>
      <Size>10</Size>
      <Style>Bold</Style>
    </Font>
  </Fonts>
  <Document>
    <Word>
      <Structure>
        <Element>
          <Paragraph>
            <FontID>Usual</FontID>
            <Parts>
              <Part>
                <Text>
                  <String>Посещаемость по буквам</String>
                </Text>
              </Part>
            </Parts>
          </Paragraph>
        </Element>
        <Element>
          <Paragraph>
            <FontID>UsualBold</FontID>
            <Parts>
              <Part>
                <Text>
                  <Placeholder>0</Placeholder>
                </Text>
              </Part>
              <Part>
                <Text>
                  <String> - </String>
                </Text>
              </Part>
              <Part>
                <Text>
                  <Placeholder>1</Placeholder>
                </Text>
              </Part>
            </Parts>
          </Paragraph>
        </Element>
        <Element>
          <Paragraph/>
        </Element>
        <Element>
          <Table>
            <AutoFit>Contents</AutoFit>
            <Placeholder>0</Placeholder>
            <Structure>
              <Column>
                <Caption>Буква</Caption>
                <CaptionFontID>UsualBoldTable</CaptionFontID>
                <DataFontID>UsualTable</DataFontID>
              </Column>
              <Column>
                <Caption>План.</Caption>
                <CaptionFontID>UsualBoldTable</CaptionFontID>
                <DataFontID>UsualTable</DataFontID>
              </Column>
              <Column>
                <Caption>Факт.</Caption>
                <CaptionFontID>UsualBoldTable</CaptionFontID>
                <DataFontID>UsualTable</DataFontID>
              </Column>
            </Structure>
          </Table>
        </Element>
      </Structure>
    </Word>
  </Document>
</Template>
